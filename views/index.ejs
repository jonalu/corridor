<!DOCTYPE html>
<html>
  <head>
    <title>Corridor</title>
    <meta charSet="utf-8" />
    <meta name="viewport" content="width=device-width, user-scalable=1, initial-scale=1" />
    <link rel="stylesheet" href="/static/style.css" />
  </head>
  <body>

    <section id="boards">
      <canvas id="game-board" width="450" height="450"></canvas>
    </section>

    <nav>
      <button><a href="/">New Game</a></button>
    </nav>

    <script>
      window.gamestate = {
        state: "<%- state %>",
        pawnPositions: <%- JSON.stringify(pawnPositions) %>,
        wallPositions: <%- JSON.stringify(wallPositions) %>,
        legalPawnMoves: <%- JSON.stringify(legalPawnMoves) %>,
        legalWallMoves: <%- JSON.stringify(legalWallMoves) %>
      }
    </script>

    <script type="text/javascript" src="/static/corridor-client.js"></script>
    <script type="text/javascript" src="/static/client.js"></script>

  </body>
</html>
